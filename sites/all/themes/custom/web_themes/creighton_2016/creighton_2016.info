name = Creighton University 2016
description = 2016 Front Page Refresh and New Colors
base theme = omega
screenshot = screenshot.png
engine = phptemplate
core = 7.x

; ========================================
; Stylesheets
; ========================================
stylesheets[all][] = css/reset.css
stylesheets[all][] = css/styles.css
stylesheets[all][] = css/hacks.css
;;; for funnelback ;;;
stylesheets[all][] = css/customer-typeahead.css

; ========================================
; Scripts
; ========================================
scripts[] = js/vendor/jquery.bxslider.min.js
;scripts[] = js/vendor/jquery.easing.1.3.js
scripts[] = js/site.js
scripts[] = js/vendor/modernizr.js
scripts[] = js/vendor/picturefill.min.js
;;; for funnelback ;;;
scripts[] = js/vendor/handlebars.js
scripts[] = js/vendor/typeahead.bundle.js
scripts[] = js/vendor/typeahead.fb.js
scripts[] = js/vendor/fitty.min.js
;scripts[] = js/cu.js
;scripts[] = js/vendor/scale.fix.js



; ========================================
; Regions
; ========================================
regions[alert_header] = Alert Header
regions[header] = Main Header
regions[secondary_header] = Secondary Header
regions[header_image] = Header Image
regions[highlighted] = Highlighted
regions[help] = Help
regions[academics_program_title] = Academics Program Title
regions[content] = Content
regions[sidebar_first] = First Sidebar
regions[sidebar_second] = Second Sidebar
regions[footer] = Footer

; Regions for Main Navigation

regions[main_nav] = Main Mega Navigation
regions[dropdown_menu] = Drop down toggle for menu

regions[header_searchbox] = Header Search
regions[header_tranlinks] = Transaction Links
regions[megamenu_dropdown] = Menu Dropdown
regions[megamenu_submenu] = Mega Menu Submenu

regions[homepage_slider] = Home Page Slider

; ========================================
; Plugins
; ========================================
plugins[panels][layouts] = panels

